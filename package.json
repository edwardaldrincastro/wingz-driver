{
  "name": "WingzDriver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --config ./.eslintrc.js --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "ios:device": "react-native run-ios --device",
    "ios:16": "react-native run-ios --simulator=\"iPhone 16 Pro  Max\"",
    "install:pods": "npx pod-install",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "clean:ios": "npm run clean:pods && cd ios && rm -rf build",
    "clean:android": "cd android/app && rm -rf build",
    "clean:modules": "rm -rf node_modules && yarn",
    "format_all": "prettier --write \"{src,tests}/**/*.{tsx,ts,json,js,jsx}\"",
    "start:json-server": "json-server --watch db.json --port 3000",
    "start:json-server-wifi": "json-server --watch db.json --host 192.168.68.107 --port 3001",
    "reset-db": "cp initial-db-state.json db.json",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/geolocation": "3.4.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "@reduxjs/toolkit": "2.2.8",
    "dayjs": "1.11.13",
    "lodash.debounce": "4.0.8",
    "patch-package": "8.0.0",
    "postinstall-postinstall": "2.1.0",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-background-timer": "2.4.1",
    "react-native-bootsplash": "6.2.1",
    "react-native-fast-image": "8.6.3",
    "react-native-geocoding": "0.5.0",
    "react-native-gesture-handler": "2.20.0",
    "react-native-get-random-values": "1.11.0",
    "react-native-google-places-autocomplete": "2.5.7",
    "react-native-maps": "1.18.0",
    "react-native-maps-directions": "1.9.0",
    "react-native-pager-view": "6.4.1",
    "react-native-progress": "5.0.1",
    "react-native-reanimated": "3.15.4",
    "react-native-safe-area-context": "4.11.0",
    "react-native-screens": "3.34.0",
    "react-native-svg": "15.7.1",
    "react-native-vector-icons": "10.2.0",
    "react-redux": "9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@testing-library/react-native": "12.7.2",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "18.3.11",
    "@types/react-native-background-timer": "2.0.2",
    "@types/react-native-vector-icons": "6.4.18",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.6.3",
    "eslint": "8.19.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.31.0",
    "jest": "29.6.3",
    "jest-transform-stub": "2.0.0",
    "json-server": "0.17.4",
    "prettier": "2.8.8",
    "react-native-dotenv": "3.4.11",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
